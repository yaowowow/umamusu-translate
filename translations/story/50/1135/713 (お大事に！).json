            "choices": [
                {
                    "jpText": "ダメ！今は休もう",
                    "enText": "No! You're resting today",
                    "nextBlock": 8
                },
                {
                    "jpText": "……わかった、許可する",
                    "enText": "……Alright, I'll let you go",
