            "nextBlock": 5,
            "origClipLength": 78,
            "voiceIdx": 37,
            "choices": [
                {
                    "jpText": "いやいや、許可できない……！",
                    "enText": "No no no, I can't let you do this…!",
                    "nextBlock": 5
